<div>

    <h1 class='card z-depth-3 center align'>Scrapin' The Buzz</h1>

    <div class="row">
        <div class="col s3"></div>
        <h5 class="card z-depth-3 center-align col s6">Click to get the latest feed.</h5>
    </div>


    <div class="row">
        <div class="col s2"></div>
        <div class="col s8 left-align">
            {{#each article}}
            <div class="card-panel">
                <h4>{{this.title}}</h4>
                <a href="/readArticle/{{this._id}}" class="waves-effect waves-light btn" id="btnSignUp">Read Article</a>
            </div>
            {{/each}}
        </div>
    </div>